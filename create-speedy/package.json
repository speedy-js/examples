{
  "name": "create-speedy",
  "version": "0.0.3",
  "description": "An easy way to create a Speedy project",
  "keywords": [
    "Speedy"
  ],
  "license": "MIT",
  "author": "Yiming Li",
  "main": "index.js",
  "bin": {
    "create-speedy": "index.js",
    "csa": "index.js"
  },
  "files": [
    "templates",
    "index.js",
    "_gitignore"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hardfist/speedystack.git",
    "directory": "packages/create-speedy"
  },
  "scripts": {
    "sync-templates": "rimraf templates && esno sync-templates.ts",
    "bundle": "esno build.ts",
    "build": "pnpm sync-templates && pnpm bundle",
    "prepublishOnly": "pnpm build",
    "dev": "pnpm bundle -- --watch",
    "test": "uvu -r tsm __tests__"
  },
  "devDependencies": {
    "@speedy-js/eslint-config": "0.1.3",
    "@types/minimist": "1.2.2",
    "@types/node": "12.20.42",
    "@types/prompts": "2.0.14",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "esbuild": "0.14.13",
    "eslint": "7.32.0",
    "esno": "0.14.0",
    "execa": "5.1.1",
    "kolorist": "1.5.1",
    "minimist": "1.2.5",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "tsm": "2.2.1",
    "type-fest": "0.21.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.4.1",
    "uvu": "0.5.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
